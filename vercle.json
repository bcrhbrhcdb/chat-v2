{
    "version": 2,
    "builds": [
      {
        "src": "pages/index.js",
        "use": "@vercel/next"
      },
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "/server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/pages/index.js"
      }
    ]
  }